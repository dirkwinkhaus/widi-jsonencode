{
  "name": "widi/json-encode",
  "description": "A light weight deep json encoder which avoids recursive encoding.",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Widi\\JsonEncode\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Widi\\JsonEncode\\": [
        "./demo",
        "./test/unit",
        "./test/integration"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6 | ^7 | ^8"
  },
  "require": {
    "psr/container": "1.0.0",
    "ext-json": "*"
  }
}
